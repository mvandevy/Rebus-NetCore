{
  "name": "Corebus",
  "version": "1.0.2-*",
  "description": "Rebus made .net core compatible. Therefore dubbed as Corebus",
  "authors": [
    "mookid8000",
    "mvandevy"
  ],
  "buildOptions": {
    "debugType": "portable"
  },
  "dependencies": {
    "Newtonsoft.Json" : "9.0.1",
    "System.Diagnostics.TraceSource": "4.3.0"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
